<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SafeAreaView">
        <attr name="bgColor" format="color"/>
        <attr name="type" format="integer"/>
    </declare-styleable>
    <declare-styleable name="TriangleView">
        <attr name="color" format="color"/>
        <attr name="heightPercent" format="float"/>
    </declare-styleable>

    <declare-styleable name="SesameCreditView">
        <!--最大数值-->
        <attr name="maxNum" format="integer"/>
        <!--圆盘起始角度-->
        <attr name="startAngle" format="integer"/>
        <!--圆盘扫过的角度-->
        <attr name="sweepAngle" format="integer"/>
        <!--测试用的信用值-->
        <attr name="currentNum" format="integer"/>
        <!-- 背景颜色 -->
        <attr name="scvBackground" format="color"/>
    </declare-styleable>
</resources>